{
    "name": "koficypher/raver",
    "description": "Unofficial Rave SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kofi Cypher",
            "email": "skcypher6@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vlucas/phpdotenv": "^3.3",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Raver\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raver\\Tests\\" : "tests" 
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8.4"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
